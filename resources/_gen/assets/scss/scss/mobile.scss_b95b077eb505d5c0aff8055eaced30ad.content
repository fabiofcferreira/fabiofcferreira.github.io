@media only screen and (max-width:1000px){#home header{overflow:auto}#home header .header-background{position:fixed}#home header #page-content{position:relative;top:0;left:0;margin:2.5rem auto;transform:none}#project-list{grid-template-columns:1fr;grid-gap:2rem 0;max-width:80%;margin:2.5rem auto 0}#project-list li a{display:flex;flex-wrap:wrap;align-items:center}#project-list li a #title{margin-bottom:0}#project-list li a #date{margin-left:auto}#project-list li #thumbnail{width:100%}}